<!-- See documentation at http://doc.nuxeo.com/x/HgIz -->
<package type="addon" name="nuxeo-multi-tenant" version="@VERSION@">
  <title>Nuxeo Multi Tenant</title>
  <description>The Nuxeo Multi-tenant addon enables to have domains, or tenants, that are independent from each other, with their own users and vocabulary values, as well as a low level fiter on content depending on tenant id. 
  </description>
  <home-page>http://doc.nuxeo.com/x/iIGo</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <!-- <hotreload-support>false</hotreload-support> -->
  <!-- <require-terms-and-conditions-acceptance>false</require-terms-and-conditions-acceptance> -->
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>production_ready</production-state>
  <supported>true</supported>
  <platforms>
    <platform>server-@DISTRIB_VERSION@</platform>
    <platform>server-@DISTRIB_VERSION@-*</platform>
  </platforms>
  <optional-dependencies>
    <package>nuxeo-jsf-ui</package>
  </optional-dependencies>
  <license>LGPL 2.1</license>
  <license-url>http://www.gnu.org/licenses/lgpl-2.1.html</license-url>
  <visibility>MARKETPLACE</visibility>
</package>
